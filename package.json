{
  "name": "tourney-tracker",
  "version": "1.0.0",
  "scripts": {
    "amplify:deploy": "amplify sandbox --once",
    "ng:serve": "ng serve ${NG_SERVE_HOST_ARG}",
    "ng:build": "ng build",
    "fix:eslint": "yarn run check:eslint --fix",
    "fix:prettier": "yarn run check:prettier --write",
    "fix": "npm-run-all --parallel --print-label 'fix:*'",
    "check:yarn:dedupe": "yarn dedupe --check",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "check:renovateconfig": "yarn dlx -p renovate --quiet renovate-config-validator --strict",
    "check:shellcheck": "shellcheck $(git ls-files | grep '.sh$')",
    "check": "npm-run-all --parallel --print-label 'check:**'",
    "all": "yarn install && yarn run check && yarn run ng:build"
  },
  "private": true,
  "engines": {
    "node": "^22.0.0"
  },
  "dependencies": {
    "@angular/animations": "18.2.12",
    "@angular/common": "18.2.12",
    "@angular/compiler": "18.2.12",
    "@angular/core": "18.2.12",
    "@angular/forms": "18.2.12",
    "@angular/platform-browser": "18.2.12",
    "@angular/platform-browser-dynamic": "18.2.12",
    "@angular/router": "18.2.12",
    "@aws-amplify/core": "6.6.0",
    "@aws-amplify/ui-angular": "5.0.32",
    "aws-amplify": "6.9.0",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.12",
    "@angular-devkit/core": "18.2.12",
    "@angular-devkit/schematics": "18.2.12",
    "@angular-eslint/builder": "18.4.1",
    "@angular-eslint/eslint-plugin": "18.4.1",
    "@angular-eslint/schematics": "18.4.1",
    "@angular-eslint/template-parser": "18.4.1",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.2.12",
    "@aws-amplify/backend": "1.8.0",
    "@aws-amplify/backend-cli": "1.4.2",
    "@aws-sdk/client-cloudformation": "3.696.0",
    "@aws-sdk/client-s3": "3.698.0",
    "@aws-sdk/client-sso-oidc": "3.699.0",
    "@aws-sdk/client-sts": "3.699.0",
    "@aws-sdk/types": "3.696.0",
    "@eslint/compat": "1.2.3",
    "@eslint/js": "9.15.0",
    "@types/jasmine": "5.1.4",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/types": "8.15.0",
    "@typescript-eslint/utils": "8.15.0",
    "angular-eslint": "18.4.1",
    "aws-cdk": "2.170.0",
    "aws-cdk-lib": "2.170.0",
    "constructs": "10.4.2",
    "eslint": "9.15.0",
    "jasmine-core": "5.4.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "npm-run-all2": "7.0.1",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tsx": "4.19.2",
    "typescript": "5.5.4",
    "typescript-eslint": "8.15.0",
    "zod": "3.23.8"
  },
  "packageManager": "yarn@4.5.2",
  "dependenciesMeta": {
    "@angular/cli": {
      "unplugged": true
    },
    "@aws-amplify/ui-angular": {
      "unplugged": true
    }
  }
